generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Room {
  id        String   @id @default(cuid())
  videoUrl  String
  isPlaying Boolean  @default(false)
  timestamp Float    @default(0)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
